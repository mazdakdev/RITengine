{
    "google_patents_search": [
        {
            "type": "function",
            "function": {
                "name": "google_patents_search",
                "description": "Get patents related to the user's query",
                "strict": true,
                "parameters": {
                    "type": "object",
                    "properties": {
                        "keyword": {"type": "string", "description": "The keyword to search for in patents"},
                        "description": {"type": "string", "description": "The description of the keyword"}
                    },
                    "required": ["keyword", "description"],
                    "additionalProperties": false
                }
            }
        }
    ],
    "google_autocomplete_search": [
        {
            "type": "function",
            "function": {
                "name": "get_autocomplete_suggestions",
                "description": "Get autocomplete suggestions for the user's query",
                "strict": true,
                "parameters": {
                    "type": "object",
                    "properties": {
                        "query": {"type": "string", "description": "The query to get autocomplete suggestions for"}
                    },
                    "required": ["query"],
                    "additionalProperties": false
                }
            }
        }
    ]
}